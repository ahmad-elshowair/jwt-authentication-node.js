{
    "name": "jwt-authentication-node.js",
    "version": "1.0.0",
    "main": "index.js",
    "repository": "git@github.com:ahmad-elshowair/jwt-authentication-node.js.git",
    "author": "Ahmad Elshowair <it.is.work@outlook.com>",
    "license": "MIT",
    "scripts": {
        "dev": "nodemon src/server.ts",
        "build": "npx tsc",
        "start": "yarn build && node dist/server.js",
        "format": "prettier --config .prettierrc --write src/**/*.ts",
        "lint": "eslint . --ext .ts",
        "lint-fix": "eslint . --ext .ts --fix",
        "jasmine": "jasmine",
        "migrate:up": "npx db-migrate up",
        "migrate:reset": "npx db-migrate reset",
        "test": "set NODE_ENV=test && db-migrate --env test up && npx tsc && jasmine && db-migrate --env test reset"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/express": "^4.17.13",
        "@types/jasmine": "^3.10.3",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/morgan": "^1.9.3",
        "@types/node": "^17.0.21",
        "@types/pg": "^8.6.5",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.14.0",
        "@typescript-eslint/parser": "^5.14.0",
        "eslint": "^8.10.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jasmine": "^4.0.2",
        "jasmine-spec-reporter": "^7.0.0",
        "jasmine-ts": "^0.4.0",
        "nodemon": "^2.0.15",
        "prettier": "^2.5.1",
        "supertest": "^6.2.2",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.2"
    },
    "dependencies": {
        "bcrypt": "^5.0.1",
        "db-migrate": "^0.11.13",
        "db-migrate-pg": "^1.2.2",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "express-rate-limit": "^6.3.0",
        "helmet": "^5.0.2",
        "jsonwebtoken": "^8.5.1",
        "morgan": "^1.10.0",
        "pg": "^8.7.3"
    }
}
